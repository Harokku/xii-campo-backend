{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 50,
    "max": 100
  },
  "authentication": {
    "secret": "SECRET",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://campia-manager.herokuapp.com/",
      "subject": "LoggedIn",
      "issuer": "Campia-Manager",
      "algorithm": "HS256",
      "expiresIn": "JWT_EXPIRE"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "mongodb": "MONGOURI"
}
